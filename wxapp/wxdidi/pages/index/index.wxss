page{
  width: 100%;
  height: 100vh;
  background-color: #f4f6f8;
}

.container{
  position: fixed;
  width: 686rpx;
  height: 438rpx;
  background-color: #fff;
  margin-top:32rpx;
  margin-left: 32rpx;
  top: 140rpx;
  -moz-box-shadow:1px 1px 6px #f4f6f8;
  -webkit-box-shadow:1px 1px 6px #f4f6f8;
  box-shadow:1px 1px 6px #f4f6f8;
}

.section{
  height:108rpx;
  width: 100%;
  background: #ffffff;
  position: fixed;
  top: 0;
  left: 0;
  position: fixed;
}

.nav {
  height:108rpx;
  width: 550rpx;
  position: fixed;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 108rpx;
  background: #ffffff;
  font-size: 16px;
  white-space: nowrap;
  top: 0;
  left: 100rpx;
  right: 100rpx;
  z-index: 99;
  margin-bottom: 32rpx;
}

.nav-me{
  height:30rpx;
  width: 30rpx;
  left: 0;
  margin-top: 10rpx;
  background-color: #fff;
  position: absolute;
  transform: scale(.15);
  z-index: 999;
}

.nav-item {
  margin-left: 20rpx;
  margin-right: 20rpx;
  display: inline-block;
  text-align: center;
}

.nav-item.active{
  color: #fc975c;
}

.nav-all{
  height:30rpx;
  width: 30rpx;
  position: absolute;
  z-index: 999;
  right: 0rpx;
  top: 0;
  background-color: #fff;
  transform: scale(.18);
  margin-right: 80rpx;
}
